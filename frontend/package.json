    "fuse.js": "^7.1.0",
    "highlight.js": "^11.11.1",
    "html2canvas": "^1.4.1",
    "html2pdf.js": "^0.10.3",
    "ioredis": "^5.8.2",
    "jspdf": "^3.0.3",
    "jszip": "^3.10.1",
    "lodash.debounce": "4.0.8",
    "lucide-react": "^0.544.0",
    "next": "15.5.3",
    "razorpay": "^2.9.6",
    "react": "19.1.0",
    "react-countup": "^6.5.3",
    "react-dom": "19.1.0",
    "react-dropzone": "^14.3.8",
    "react-focus-lock": "^2.13.6",
    "react-hook-form": "^7.63.0",
    "react-markdown": "^10.1.0",
    "react-window": "^2.1.1",
    "react-window-infinite-loader": "^2.0.0",
    "recharts": "^3.2.1",
    "rehype-highlight": "^7.0.2",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "reveal.js": "^5.2.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.13",
    "zod": "3.25.76",
    "zustand": "^4.5.7"
  },
  "resolutions": {
    "ai": "5.0.0",
    "@ai-sdk/anthropic": "1.0.0",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.5.4",
    "@tanstack/react-query-devtools": "^5.90.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/file-saver": "^2.0.7",
    "@types/glob": "^8.1.0",
    "@types/html2pdf.js": "^0.10.0",
    "@types/jsdom": "^21.1.7",
    "@types/jszip": "^3.4.0",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/coverage-v8": "^3.2.4",
    "critters": "^0.0.23",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "eslint-config-prettier": "^9",
    "eslint-plugin-prettier": "^5",
    "glob": "^11.0.3",
    "husky": "^9",
    "jsdom": "^27.0.0",
    "lint-staged": "^15",
    "prettier": "^3",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "sharp": "^0.34.4",
    "tsx": "^4.19.2",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --max-warnings=-1",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
