{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:integration": "vitest run",
    "types:gen": "supabase gen types typescript --project-id $SUPABASE_PROJECT_REF --schema public > ./types/supabase.ts || echo 'Using hand-written types'",
    "test:watch": "vitest",
    "cleanup-duplicates": "tsx scripts/cleanup-duplicates.ts",
    "create-razorpay-plans": "tsx scripts/create-razorpay-plans.ts",
    "create-new-plans": "tsx scripts/createRazorpayPlans.ts",
    "test-webhook-endpoint": "tsx scripts/test-webhook-endpoint.ts",
    "setup-dev-plans": "tsx scripts/setup-dev-plans.ts",
    "list-razorpay-plans": "tsx scripts/list-razorpay-plans.ts",
    "delete-old-razorpay-plans": "tsx scripts/delete-old-razorpay-plans.ts",
    "delete-all-razorpay-plans": "tsx scripts/delete-all-razorpay-plans.ts",
    "load-test": "tsx scripts/run-load-test.ts",
    "load-test:pricing": "tsx scripts/run-load-test.ts run --preset pricingPage",
    "load-test:subscriptions": "tsx scripts/run-load-test.ts run --preset subscriptionCreation",
    "load-test:blueprints": "tsx scripts/run-load-test.ts run --preset blueprintGeneration",
    "load-test:health": "tsx scripts/run-load-test.ts health-check",
    "analyze:bundle": "ANALYZE=true npm run build",
    "validate:production": "tsx scripts/validate-production-config.ts",
    "env:validate": "tsx scripts/validate-production-config.ts",
    "db:migrate": "cd .. && ./scripts/deploy-database-migrations.sh",
    "db:verify": "tsx ../scripts/verify-database-migrations.ts",
    "db:migrate:prod": "cd .. && ./scripts/deploy-database-migrations.sh production",
    "db:verify:prod": "tsx ../scripts/verify-database-migrations.ts production",
    "deploy:checklist": "tsx ../scripts/production-deployment-checklist.ts",
    "deploy:checklist:prod": "tsx ../scripts/production-deployment-checklist.ts production",
    "smoke:tests": "tsx ../scripts/production-smoke-tests.ts",
    "smoke:tests:prod": "tsx ../scripts/production-smoke-tests.ts",
    "smoke:tests:health": "tsx ../scripts/production-smoke-tests.ts --suite=health",
    "smoke:tests:auth": "tsx ../scripts/production-smoke-tests.ts --suite=authentication",
    "smoke:tests:payments": "tsx ../scripts/production-smoke-tests.ts --suite=payment",
    "incident:respond": "tsx ../scripts/incident-response-automation.ts",
    "incident:test": "tsx ../scripts/incident-response-automation.ts low test \"Test incident notification system\"",
    "emergency:contacts": "cat ../scripts/emergency-contacts-config.json",
    "monitor:24hour": "cd .. && ./scripts/24hour-monitoring.sh",
    "monitor:24hour:6h": "cd .. && ./scripts/24hour-monitoring.sh 6",
    "monitor:24hour:1h": "cd .. && ./scripts/24hour-monitoring.sh 1",
    "prepare": "husky",
    "test:redis": "tsx scripts/test-redis.ts",
    "setup:vercel-kv": "tsx scripts/setup-vercel-kv.ts"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "1.0.0",
